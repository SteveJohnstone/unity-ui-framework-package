.unity-min-max-slider .unity-min-max-slider__min-thumb, .unity-min-max-slider .unity-min-max-slider__max-thumb {
    border-radius: 8px;
    width: 16px;
    height: 16px;
    background-color: var(--color-primary);
    margin-top:-8px;
}

.unity-base-field {
    /*flex-direction: col;*/
}

.container {
    margin: 8px;
    padding: 8px;
    background-color: rgb(34, 34, 34);
}

.header {
    font-size: 32px;
    -unity-font-style: bold;
}

.header.h2 {
    font-size: 28px;
}

.card {
    border-width: 1px;
    border-color: rgba(0, 0, 0, 0.13);
    border-radius: var(--border-radius);
    background-color: rgb(48, 48, 48);
}

.card-header {
    padding-top: var(--p-2);
    padding-bottom: var(--p-2);
    padding-left: var(--p-3);
    padding-right: var(--p-3);
}

.card-body {
    padding-top: var(--p-2);
    padding-bottom: var(--p-2);
    padding-left: var(--p-3);
    padding-right: var(--p-3);
}

.card-footer {
    padding-top: var(--p-2);
    padding-bottom: var(--p-2);
    padding-left: var(--p-3);
    padding-right: var(--p-3);
}

.card-header {
    border-bottom-color: rgba(0, 0, 0, 0.13);
    border-bottom-width: 1px;
}

.card-footer {
    border-top-color: rgba(0, 0, 0, 0.13);
    border-top-width: 1px;
}

.card-header {
    background-color: rgb(68, 68, 68);
}

.card-footer {
    background-color: rgb(68, 68, 68);
}

.card-header .header {
    font-size: 20px;
    margin: 0;
    padding: 0;
}

.btn {
    border-width: 1px;
    border-color: rgba(0, 0, 0, 0);
    border-radius: var(--border-radius);
    padding-top: 6px;
    padding-bottom: 6px;
    padding-left: 12px;
    padding-right: 12px;
    background-color: var(--color-light);
    border-color: var(--color-light);
    font-size: 16px;
}

.btn.btn-primary {
    background-color: var(--color-primary);
    border-color: var(--color-primary);
}

.btn.btn-secondary {
    background-color: var(--color-secondary);
    border-color: var(--color-secondary);
}

.btn.btn-success {
    background-color: var(--color-success);
    border-color: var(--color-success);
}

.btn.btn-info {
    background-color: var(--color-info);
    border-color: var(--color-info);
}

.btn.btn-warning {
    background-color: var(--color-warning);
    border-color: var(--color-warning);
}

.btn.btn-danger {
    background-color: var(--color-danger);
    border-color: var(--color-danger);
}

.btn.btn-light {
    background-color: var(--color-light);
    border-color: var(--color-light);
}

.btn.btn-dark {
    background-color: var(--color-dark);
    border-color: var(--color-dark);
}

.btn.btn-link {
    border-width: 0;
    background-color: rgba(0, 0, 0, 0);
}

.row {
    flex-direction: row;
    margin-bottom: 8px;
}
.col {
    margin-left: 12px;
    margin-right: 12px;
}

.alert {
    padding: 16px;
    border-radius: var(--border-radius);
    flex-grow: 1;
}

.alert .header {
    font-size: 20px;
    margin: 0;
    padding: 0;
}

.alert.alert-primary {
    background-color: var(--color-primary);
    border-color: var(--color-primary);
}

.alert.alert-secondary {
    background-color: var(--color-secondary);
    border-color: var(--color-secondary);
}

.alert.alert-success {
    background-color: var(--color-success);
    border-color: var(--color-success);
}

.alert.alert-info {
    background-color: var(--color-info);
    border-color: var(--color-info);
}

.alert.alert-warning {
    background-color: var(--color-warning);
    border-color: var(--color-warning);
}

.alert.alert-danger {
    background-color: var(--color-danger);
    border-color: var(--color-danger);
}

.alert.alert-light {
    background-color: var(--color-light);
    border-color: var(--color-light);
}

.alert.alert-dark {
    background-color: var(--color-dark);
    border-color: var(--color-dark);
}

.unity-ui-document__root{
    width: 100%;
    height: 100%;
}